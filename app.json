{
    "name": "papa chatbot", 
    "logo": "https://graph.org/file/6b1e6a6b8b0bb4934689f.jpg",
    "description": "An Advanced Telegram UserId Chatbot.",
    "env": {
        "STRING": {
            "description": "Pyrogram String session of your account",
            "required": true
        },        
        "MONGO_URL": {
            "description": "mongo db url here",
            "required": true
        },
        "API_ID": {
            "description": "telegram api id here",
            "required": true
        },
        "API_HASH": {
            "description": "telegram api hash here",
            "required": true
        }
    },
    "addons": [
        {
            "plan": "heroku-postgresql"
        }
    ]
}
